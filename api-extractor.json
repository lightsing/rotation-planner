{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",

    "mainEntryPointFilePath": "./dist/index.d.ts",
    "bundledPackages": [],
    "newlineKind": "lf",
    "apiReport": {
        "enabled": true,
        "reportFileName": "base.api.md",
        "reportFolder": "./api/",
        "reportTempFolder": "<projectFolder>/dist/api-extractor-base/"
    },
    "docModel": {
        "enabled": false
    },
    "dtsRollup": {
        "enabled": true,
        "untrimmedFilePath": "./dist/index.d.ts"
    },
    "tsdocMetadata": {
        "enabled": false
    }
}